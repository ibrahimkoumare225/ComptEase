<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.AmbientLight?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fr.koumare.comptease.controllers.ObligationFiscaleController">

    <TableView fx:id="obligationTable" layoutX="20" layoutY="20" prefWidth="600" prefHeight="300">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" prefWidth="50"/>
            <TableColumn fx:id="montantColumn" text="Montant" prefWidth="100"/>
            <TableColumn fx:id="echeanceColumn" text="Échéance" prefWidth="150"/>
            <TableColumn fx:id="typeColumn" text="Type d'impôt" prefWidth="100"/>
        </columns>
    </TableView>

    <Button layoutX="20" layoutY="340" text="Supprimer" onAction="#handleSupprimer"/>
    <Button layoutX="120" layoutY="340" text="Modifier" onAction="#handleModifier"/>

</AnchorPane>
